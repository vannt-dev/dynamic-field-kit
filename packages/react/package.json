{
    "name": "@dynamic-field-kit/react",
    "version": "1.0.1",
    "description": "React renderer for dynamic-field-kit",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "react": ">=17"
    },
    "dependencies": {
        "@dynamic-field-kit/core": "^1.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist"
    }
}
