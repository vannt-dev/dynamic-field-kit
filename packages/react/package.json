{
    "name": "@dynamic-field-kit/react",
    "version": "1.0.6",
    "description": "React renderer for dynamic-field-kit",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "dependencies": {
        "@dynamic-field-kit/core": "^1.0.3"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist"
    },
    "repository": "https://github.com/vannt-dev/dynamic-field-kit.git",
    "keywords": [
        "dynamic-field",
        "dynamic-field-kit",
        "dynamic-field-kit/react"
    ]
}
